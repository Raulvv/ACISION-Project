<h1>Listing tracks</h1>
  <p>Audio tracks</p>


<table>
    <% @tracks.each do |track| %>
      <tr>
        <td><audio id='<%= session.id %>' class="player" src='<%= track.audio %>' controls></audio></td>
        <td><%= track.name %></td>
        <td><%= link_to 'Show', track %></td>
        <td><%= link_to 'Edit', edit_track_path(track) %></td>
        <td><%= link_to 'Delete', track, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
</table>

<br>

<%= link_to 'New Track', new_track_path %>
